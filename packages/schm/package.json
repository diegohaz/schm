{
  "name": "schm",
  "version": "0.3.4",
  "description": "Composable, immutable, parseable and validatable (yeah, many *ables) schemas for Node/JavaScript",
  "license": "MIT",
  "repository": "https://github.com/diegohaz/schm/tree/master/packages/schm",
  "main": "dist/index.js",
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "flow": "flow",
    "docs": "documentation readme src --section=API --pe flow -c documentation.yml",
    "clean": "rimraf dist",
    "prebuild": "npm run docs && npm run clean",
    "build": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "schm",
    "schema",
    "scheme",
    "parser",
    "validator"
  ],
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "documentation": "^6.0.0",
    "eslint": "^4.15.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.66.0",
    "humps": "^2.0.1",
    "jest-cli": "^22.0.6",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.1",
    "validate.js": "^0.12.0"
  }
}
