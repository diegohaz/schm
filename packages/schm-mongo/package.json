    "postpublish": "npm run clean"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "schm",
    "schema",
    "scheme",
    "parser",
    "validator",
    "mongo",
    "mongodb"
  ],
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "documentation": "vicapow/documentation#a560a4373c301e37251f13e045811932c9c4ac01",
    "eslint": "^5.5.0",
    "flow-bin": "^0.80.0",
    "jest-cli": "^23.6.0",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.1",
    "schm": "^0.4.0"
  },
  "peerDependencies": {
    "schm": "^0.4.0"
  }
}
